	pages = {12--24},
	file = {1-s2.0-S0273230016302616-main.pdf:/home/grace/Zotero/storage/PHACVDVS/1-s2.0-S0273230016302616-main.pdf:application/pdf;Accepted Version:/home/grace/Zotero/storage/NKMEAP2R/Shah et al. - 2016 - Systematically evaluating read-across prediction a.pdf:application/pdf},
