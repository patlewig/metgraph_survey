	year = {2016},
	pmid = {27174420},
	keywords = {Animals, Humans, Quantitative Structure-Activity Relationship, Read-across, Risk Assessment, Toxicity Tests, Animal Testing Alternatives, Biological Assay, Data Mining, Molecular Structure, Reproducibility of Results, ToxCast, (Q)SAR, Environmental Pollutants, Pharmaceutical Preparations, Algorithms, Cluster Analysis, Bioactivity, KNN, Local validity domains, Nearest neighbors, Databases, Automated, Factual, Pattern Recognition},
	pages = {12--24},
