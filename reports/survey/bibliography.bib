	keywords = {Computational toxicology, ToxCast, ToxRefDB, Bioassay data, Compound database, Data curation, EDSP21, Environmental chemistry, Environmental fate and transport data, Non-targeted analysis, Open data, Physicochemical properties, Toxic Substances Control Act (TSCA), Toxicity data},
	pages = {61},
