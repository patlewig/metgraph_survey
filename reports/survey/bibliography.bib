	pmid = {32388568},
	keywords = {Humans, read-across, Risk Assessment, Hazardous Substances, Structure-Activity Relationship, Animal Testing Alternatives, biological similarity, Reproducibility of Results, workflow, Algorithms, data-gap filling},
	pages = {469--481},
	file = {Full Text:/home/grace/Zotero/storage/8RPAZZEZ/Gadaleta et al. - 2020 - Automated integration of structural, biological an.pdf:application/pdf},
